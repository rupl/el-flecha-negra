---
---
{"data": [
  {% for event in site.events %}
    {
      "title": "{{ event.title | escape }}",
      "where": "{{ event.where | escape }}",
      "date": "{{ event.date | escape }}",
      "time": "{{ event.time | escape }}",
      "more-info": "{{ event.more-info | escape }}",
      "map": "{{ event.map | escape }}",
      "published": {% if event.published == false %}false{% else %}true{% endif %}
    },
  {% endfor %}
  {}
]}
